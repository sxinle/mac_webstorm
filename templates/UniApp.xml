<templateSet group="UniApp">
  <template name="ifdef" value="&lt;!-- #ifdef $types$ --&gt;&#10;    $END$&#10;&lt;!-- #endif --&gt;&#10;" description="" toReformat="false" toShortenFQNames="true">
    <variable name="types" expression="enum(&quot;APP-PLUS&quot;, &quot;APP-VUE&quot;, &quot;APP-NVUE&quot;, &quot;MP&quot;, &quot;MP-ALIPAY&quot;, &quot;MP-BAIDU&quot;, &quot;MP-WEIXIN&quot;, &quot;MP-QQ&quot;, &quot;H5&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="ifndef" value="&lt;!-- #ifndef $types$ --&gt;&#10;    $END$&#10;&lt;!-- #endif --&gt;&#10;" description="" toReformat="false" toShortenFQNames="true">
    <variable name="types" expression="enum(&quot;APP-PLUS&quot;, &quot;APP-VUE&quot;, &quot;APP-NVUE&quot;, &quot;MP&quot;, &quot;MP-ALIPAY&quot;, &quot;MP-BAIDU&quot;, &quot;MP-WEIXIN&quot;, &quot;MP-QQ&quot;, &quot;H5&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="vbase" value="&lt;template&gt;&#10;&#9;&lt;$div$&gt;&#10;&#10;&#9;&lt;/$div$&gt;&#10;&lt;/template&gt;&#10;&#10;&lt;script&gt;&#10;&#9;export default {&#10;&#9;&#9;$END$&#10;&#9;}&#10;&lt;/script&gt;&#10;&#10;&lt;style scoped&gt;&#10;&#10;&lt;/style&gt;&#10;" description="Base for Vue File" toReformat="false" toShortenFQNames="true">
    <variable name="div" expression="" defaultValue="&quot;div&quot;" alwaysStopAt="true" />
    <context>
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="vnuxtl" value="&lt;nuxt-link to=&quot;/$page$&quot;&gt;$page$&lt;/nuxt-link&gt;$END$&#10;" description="nuxt routing link" toReformat="false" toShortenFQNames="true">
    <variable name="page" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="von" value="@click=&quot;handler(arg, $event$)&quot;$END$&#10;" description="v-on click handler with arguments" toReformat="false" toShortenFQNames="true">
    <variable name="event" expression="" defaultValue="&quot;event&quot;" alwaysStopAt="true" />
    <context>
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="uAccordion" value="&lt;view class=&quot;uni-list&quot;&gt;&#10;&#9;&lt;view class=&quot;uni-list-cell uni-collapse&quot; v-for=&quot;(item,index) in $list$&quot; :key=&quot;index&quot;&gt;&#10;&#9;&#9;&lt;view class=&quot;uni-list-cell-navigate uni-navigate-bottom&quot; :class=&quot;item.show ? 'uni-active' : ''&quot; @click=&quot;trigerCollapse$END$(index)&quot;&gt;&#10;&#9;&#9;&#9;折叠面板{{index}}&#10;&#9;&#9;&lt;/view&gt;&#10;&#9;&#9;&lt;view class=&quot;uni-collapse-content&quot; :class=&quot;item.show ? 'uni-active' : ''&quot;&gt;&#10;&#9;&#9;&#9;&lt;view class=&quot;page-pd&quot;&gt;&#10;&#9;&#9;&#9;&#9;&lt;view class=&quot;uni-h1&quot;&gt;hello uni-app&lt;/view&gt;&#10;&#9;&#9;&#9;&#9;&lt;view class=&quot;uni-h2&quot;&gt;hello uni-app&lt;/view&gt;&#10;&#9;&#9;&#9;&#9;&lt;view class=&quot;uni-h3&quot;&gt;hello uni-app&lt;/view&gt;&#10;&#9;&#9;&#9;&lt;/view&gt;&#10;&#9;&#9;&lt;/view&gt;&#10;&#9;&lt;/view&gt;&#10;&lt;/view&gt;&#10;" toReformat="false" toShortenFQNames="true">
    <variable name="list" expression="" defaultValue="&quot;list&quot;" alwaysStopAt="true" />
    <context>
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="uAudio" value="&lt;audio :src=&quot;$src$&quot; :poster=&quot;$poster$&quot; :name=&quot;$name$&quot; :author=&quot;$author$&quot; :action=&quot;$action$&quot; controls&gt;&lt;/audio&gt;&#10;" toReformat="false" toShortenFQNames="true">
    <variable name="src" expression="" defaultValue="&quot;src&quot;" alwaysStopAt="true" />
    <variable name="poster" expression="" defaultValue="&quot;poster&quot;" alwaysStopAt="true" />
    <variable name="name" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true" />
    <variable name="author" expression="" defaultValue="&quot;author&quot;" alwaysStopAt="true" />
    <variable name="action" expression="" defaultValue="&quot;action&quot;" alwaysStopAt="true" />
    <context>
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="uBadge" value="&lt;uni-badge text=&quot;$text$&quot; type=&quot;$type$&quot;&gt;&lt;/uni-badge&gt;&#10;" toReformat="false" toShortenFQNames="true">
    <variable name="text" expression="" defaultValue="&quot;text&quot;" alwaysStopAt="true" />
    <variable name="type" expression="" defaultValue="&quot;type&quot;" alwaysStopAt="true" />
  </template>

  <template name="ucheckbox" value="&lt;label&gt;&amp;#10;&amp;#9;&lt;checkbox :value=&quot;$1$&quot; /&gt;&lt;text&gt;$0$&lt;/text&gt;&amp;#10;&lt;/label&gt;&amp;#10;" description="" toReformat="false" toShortenFQNames="true">
      <variable name="1" expression="" defaultValue="1" alwaysStopAt="true"/>
      <variable name="0" expression="" defaultValue="0" alwaysStopAt="true"/>
      <context>
          <option name="Vue" value="true"/>
      </context>
  </template>

</templateSet>
